{
  "name": "sosam",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "ext": "js",
    "ignore": [
      "*.test.ts",
      "db/*"
    ],
    "delay": 2
  },
  "scripts": {
    "test": "jest --forceExit",
    "test:silent": "jest --silent --forceExit",
