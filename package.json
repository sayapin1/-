    "express": "~4.18.2",
    "http": "^0.0.1-security",
    "init": "^0.1.2",
    "jest-mock-req-res": "^1.0.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.1.0",
    "nodemon": "^2.0.20",
    "openai": "^3.1.0",
    "req": "^0.1.4",
    "sequelize": "^6.28.0",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "jest": "^29.4.3",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  }
